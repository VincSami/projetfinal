"use strict";var urlAddress=window.location!=window.parent.location?document.referrer:document.location.href,basePath="https://ikeacanada.blue.cdtapps.com/local-store-seo/",urlString=urlAddress.replace("https://m2.ikea.com/ca/en/stores/","");if(-1!==(urlString=urlString.replace("https://m2.ikea.com/ca/fr/stores/","")).indexOf("-store"))loadScript(urlPath=urlString.substring(0,urlString.indexOf("-store")));else if(-1!==urlString.indexOf("-pup")){loadScript(urlPath=urlString.substring(0,urlString.indexOf("-pup")))}else if(-1!==urlString.indexOf("-collection")){loadScript(urlPath=urlString.substring(0,urlString.indexOf("-collection")))}else if(-1!==urlString.indexOf("-pub")){var urlPath;loadScript(urlPath=urlString.substring(0,urlString.indexOf("-pub")))}function loadScript(r){var t=document.createElement("script");t.type="application/ld+json",t.src=basePath+r+".js",document.getElementsByTagName("head")[0].appendChild(t),console.log("Local SEO script added")}